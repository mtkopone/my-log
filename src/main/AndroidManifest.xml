<manifest 
android:versionName="0.1" android:versionCode="1" package="mtk.mylog" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="4"></uses-sdk>
    <application android:icon="@android:drawable/sym_action_call" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".RecentLogActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
</manifest>
